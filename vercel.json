{
  "version": 2,
  "builds": [
    {
      "src": "api/mcp.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mcp/(.*)",
      "dest": "/api/mcp.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/mcp.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/mcp.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
